{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "env-cmd -f .env.development gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "mybuild": "env-cmd -f .env.development gatsby build && gatsby serve",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-relative-images": "^0.2.3",
    "gatsby-source-contentful": "^2.1.96",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^5.25.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "ptz-i18n": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.7.2",
    "react-typography": "^0.16.19",
    "slash": "^3.0.0",
    "typography": "^0.16.19",
    "typography-breakpoint-constants": "^0.16.19"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
